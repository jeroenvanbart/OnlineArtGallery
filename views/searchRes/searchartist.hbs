{{#if foundArtArr}}
<h1 class="headerbox">We found these artworks for you based upon you artists search</h1>
{{else}}
<h1 class="headerbox">Unfortunately we couldn't find artist matching your search</h1>
{{/if}}
<section class="allartcontainersearch">
    {{#each foundArtArr}}
    <div class="artcardssearch">
        <h2> {{this.owner.fullName}} </h2>
        <h2> {{this.title}} </h2>
        <div class="artimageallart">
            <img src="{{this.imageUrl}}" alt="">
        </div>
        <p>See artist profile</p>
        <a href="{{this.owner.id}}/details">See Details</a>
    </div>
    {{/each}}
</section>